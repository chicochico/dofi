        "rules": [
          {
            "description": "Space alone otherwise left control",
            "manipulators": [
              {
                "from": {
                  "key_code": "spacebar",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "left_control"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "spacebar"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "[ alone otherwise left shift",
            "manipulators": [
              {
                "from": {
                  "key_code": "left_shift",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "left_shift"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "open_bracket"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "] alone otherwise right shift",
            "manipulators": [
              {
                "from": {
                  "key_code": "right_shift",
                  "modifiers": {
                    "optional": [
                      "any"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "right_shift"
                  }
                ],
                "to_if_alone": [
                  {
                    "key_code": "close_bracket"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Focus Alacritty",
            "manipulators": [
              {
                "from": {
                  "key_code": "a",
                  "modifiers": {
                    "mandatory": [
                      "left_control",
                      "left_command"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a Alacritty"
                  }
                ],
                "type": "basic"
              }
            ]
          },
          {
            "description": "Focus Firefox",
            "manipulators": [
              {
                "from": {
                  "key_code": "s",
                  "modifiers": {
                    "mandatory": [
                      "left_control",
                      "left_command"
                    ]
                  }
                },
                "to": [
                  {
                    "shell_command": "open -a Firefox"
                  }
                ],
                "type": "basic"
              }
            ]
          }
        ]
      },
      "devices": [],
